    <div class="container">


<!-- Forms
================================================== -->
<section id="forms">
  <div class="page-header">
    <h1>Enter the highway......</h1>
  </div>

  <div class="row">
    <div class="span7 offset1">
      <form class="form-horizontal well" action="/session" method="POST">
        <fieldset>
          <legend>Enter the Danger Zone......</legend>
          <div class="control-group">
            <label class="control-label" for="select01">Age</label>
            <div class="controls">
              <select name="age">
                <% Factors::AgeGenderFactor::AGE_RISK.keys.each do |age| %>
                <option value='<%=age%>'><%=age%></option>
                <% end %>
              </select>
            </div>
          </div>
		            <div class="control-group">
            <label class="control-label" for="select01">Sex</label>
            <div class="controls">
              <select name="gender">
                <% Factors::AgeGenderFactor::GENDERS.each do |gender| %>
                <option value='<%=gender%>'><%=gender%></option>
                <% end %>
              </select>
            </div>
          </div>
		            <div class="control-group">
            <label class="control-label" for="select01">Are you drunk?</label>
            <div class="controls">
              <select id="select01">
                <option>No</option>
                <option>Yes</option>
              </select>
            </div>
          </div>
          <div class="form-actions">
            <button type="submit" class="btn btn-primary">I want more danger!</button>
            <button type="reset" class="btn">Cancel</button>
          </div>
        </fieldset>
      </form>
    </div>
  </div>
  
</section>

</section>

<br><br><br><br>

     <!-- Footer
      ================================================== -->
      <footer class="footer">
        <p class="pull-right"><a href="#">Back to top</a></p>
      </footer>

    </div><!-- /container -->



